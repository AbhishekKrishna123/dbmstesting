    <section class="container" style="margin-top: 5em">

        <h3> Upcoming Tests </h3>

        {{!-- <ul>
            {{#each Tests}}
                <li> {{this.Name}} </li>
            {{/each}}
        </ul> --}}
        <table class="bordered highlight ">
            <thead>
                <tr>
                    <th>
                        Company Name
                    </th>
                    <th>
                        Test Date
                    </th>
                    <th>
                        Test Time
                    </th>
                </tr>
            </thead>
            <tbody>
                {{#each UnregTests}}
                    <tr>
                        <td> {{this.Name}} </td>
                        <td> {{this.TestDate}} </td>
                        <td> {{this.TestTime}} </td>
                        <td style = "color: red"> Not Registered! </td>
                        <td> <button class="waves-effect waves-light btn green" onclick = "reg(this)" name = {{this.TestID}}> Register </button> </td>
                    </tr>
                {{/each}}
                {{#each RegTests}}
                    <tr>
                        <td> {{this.Name}} </td>
                        <td> {{this.TestDate}} </td>
                        <td> {{this.TestTime}} </td>
                        {{!-- <td> <button class="waves-effect waves-light btn blue" onclick = "reg(this)" name = {{this.TestID}}> Register </button> </td> --}}
                        <td style = "color: green"> Already Registered </td>
                        <td> <button class="waves-effect waves-light btn red" onclick = "unreg(this)" name = {{this.TestID}}> Unregister </button> </td>
                    </tr>
                {{/each}}

                {{#each IneligibleTests}}
                    <tr>
                        <td> {{this.Name}} </td>
                        <td> {{this.TestDate}} </td>
                        <td> {{this.TestTime}} </td>
                        {{!-- <td> <button class="waves-effect waves-light btn blue" onclick = "reg(this)" name = {{this.TestID}}> Register </button> </td> --}}
                        <td style = "color: black"> Cannot Register </td>
                        <td> Inadequte GPA </td>

                    </tr>
                {{/each}}



            </tbody>
        </table>
    </section>

    <br><br>

        <script src="js/vendor/modernizr-3.5.0.min.js"></script>
