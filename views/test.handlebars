
    <section class="container row" style="margin-top: 3em">

        <div class="container col s12 center">

            <h4 class="teal-text center">View Test Details</h4>

            {{!-- <ul>
                {{#each Tests}}
                    <li> {{this.Name}} </li>
                {{/each}}
            </ul> --}}
            {{#each Test}}
                    <h4 class="blue-text">{{this.Name}}</h4>
                    <div class="row">
                        <p class="grey-text col s6">Date: {{this.TestDate}}</p>
                        <p class="grey-text col s6">Time: {{this.TestTime}}</p>
                        <p class="grey-text col s6">Location: {{this.Location}}</p>
                        <p class="grey-text col s6">Cut-Off: {{this.CutOffGPA}}</p>
                    </div>
            {{/each}}

        </div>

        <div class="col s12">
            <h4 class="teal-text center">Registered Students</h4>
            <br>
            <table class="bordered highlight ">
                <thead>
                    <tr>
                        <th>USN</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>CGPA</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each Reg}}
                    <tr>
                        <td>{{this.USN}}</td>
                        <td>{{this.FirstName}}</td>
                        <td>{{this.LastName}}</td>
                        <td>{{this.CGPA}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>

    </section>

    <div class="center">
        <br><br>
        <a class="waves-effect waves-light btn blue" href="/logout"> Logout </a>
    </div>

    <div class="center">
        <br><br>
        <a class="waves-effect waves-light btn blue" href="/password_change"> Change Password </a>
    </div>

    <br><br>

        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <script src="./materialize/js/materialize.min.js"></script>

        <script>

            $(document).ready(function() {
                $(".button-collapse").sideNav();
            });

        </script>
    </body>
</html>
