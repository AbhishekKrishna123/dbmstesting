<section class="container row">

    <div class="container col s12 center">

        <h4 class="teal-text center">View Test Details</h4>
        <h6 class = "grey-text center">For students of your department only</h6>

        <br>

        {{#each Test}}
        <div class="col s8 offset-s2">
            <div class="card blue-grey darken-3">
                <div class="card-content white-text">
                    <span class="card-title">{{this.Name}}</span>
                    <h5> Conducted by: {{this.CompanyName}} </h5>
                    <p> Date: {{this.TestDate}} </p>
                    <p> Time: {{this.TestTime}} </p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>

    <br>

    <div class="col s12 l6 left">
        <h4 class="teal-text center">Registered Students</h4>
        <br>
        <table class="bordered highlight">
            <thead>
                <tr>
                    <th>USN</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>CGPA</th>
                </tr>
            </thead>
            <tbody>
                {{#each Reg}}
                <tr>
                    <td>{{this.USN}}</td>
                    <td>{{this.FirstName}}</td>
                    <td>{{this.LastName}}</td>
                    <td>{{this.CGPA}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    <br>
    <br>
    <div class="col s12 l6 right">
        <h4 class="teal-text center">Selected Students</h4>
        <br>
        <table class="bordered highlight center">
            <thead>
                <tr>
                    <th>USN</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>CGPA</th>
                </tr>
            </thead>
            <tbody>
                {{#each Selected}}
                <tr>
                    <td>{{this.USN}}</td>
                    <td>{{this.FirstName}}</td>
                    <td>{{this.LastName}}</td>
                    <td>{{this.CGPA}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>

</section>
<section class="container" style="margin-top: 5em">

    <div class="row center">
        <div class="col s12">
            <a class="waves-effect waves-light btn teal" href="/dashboard"> Back </a>
        </div>
    </div>
</section>

