<section class="container row" style="margin-top: 3em">

    <div class="container col s12 center">

        <h4 class="teal-text center">View Test Details</h4>

        {{!--
        <ul>
            {{#each Tests}}
            <li> {{this.Name}} </li>
            {{/each}}
        </ul> --}}  {{!--
        <h4 class="blue-text">{{this.Name}}</h4>
        <div class="row">
            <p class="grey-text col s6">Date: {{this.TestDate}}</p>
            <p class="grey-text col s6">Time: {{this.TestTime}}</p>
            <p class="grey-text col s6">Location: {{this.Location}}</p>
            <p class="grey-text col s6">Cut-Off: {{this.CutOffGPA}}</p>
        </div> --}}
        {{#each Test}}
        <div class="col s8 offset-s2">
            <div class="card blue-grey darken-3">
                <div class="card-content white-text">
                    <span class="card-title">{{this.Name}}</span>
                    <h5> Conducted by: {{this.CompanyName}} </h5>
                    <p> Date: {{this.TestDate}} </p>
                    <p> Time: {{this.TestTime}} </p>
                </div>
            </div>
        </div>
        {{/each}}
        <br>
    </div>
    {{!-- <div class = "container"></div> --}}
    <div class="col s12 l6 left" style="padding-left: 3em;">
        <h4 class="teal-text center">Registered Students</h4>
        <br>
        <table class="bordered highlight ">
            <thead>
                <tr>
                    <th>USN</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>CGPA</th>
                </tr>
            </thead>
            <tbody>
                {{#each Reg}}
                <tr>
                    <td>{{this.USN}}</td>
                    <td>{{this.FirstName}}</td>
                    <td>{{this.LastName}}</td>
                    <td>{{this.CGPA}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    <br>
    <br>
    <div class="col s12 l6 right" style="padding-left: 3em;">
        <h4 class="teal-text center">Selected Students</h4>
        <br>
        <table class="bordered highlight ">
            <thead>
                <tr>
                    <th>USN</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>CGPA</th>
                </tr>
            </thead>
            <tbody>
                {{#each Selected}}
                <tr>
                    <td>{{this.USN}}</td>
                    <td>{{this.FirstName}}</td>
                    <td>{{this.LastName}}</td>
                    <td>{{this.CGPA}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>

</section>
<section class="container" style="margin-top: 5em">

    <div class="row">
        <div class="col s2 offset-s5">
            <a class="waves-effect waves-light btn teal" href="/dashboard"> Back </a>
        </div>
    </div>
</section>

